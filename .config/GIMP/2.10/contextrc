# GIMP user context

(tool "gimp-rect-select-tool")
(paint-info "gimp-paintbrush")
(foreground (color-rgb 1.000000 0.000000 0.000000))
(paint-mode color-erase)
(brush "2. Hardness 100")
(dynamics "Dynamics Off")
(mybrush "100% Opaque")
(pattern "Amethyst")
(gradient "FG to BG (RGB)")
(palette "Color History")
(font "Sans-serif")
(tool-preset "Core Pointer")

# end of user context
